[MASTER]
ignore=CVS
persistent=yes
load-plugins=
jobs=1
unsafe-load-any-extension=no
extension-pkg-whitelist=
optimize-ast=no

[MESSAGES CONTROL]
confidence=
disable=long-suffix,raw_input-builtin,xrange-builtin,metaclass-assignment,cmp-builtin,next-method-called,long-builtin,execfile-builtin,using-cmp-argument,backtick,indexing-exception,parameter-unpacking,standarderror-builtin,print-statement,cmp-method,filter-builtin-not-iterating,delslice-method,input-builtin,basestring-builtin,import-star-module-level,coerce-method,useless-suppression,nonzero-method,reduce-builtin,file-builtin,buffer-builtin,zip-builtin-not-iterating,dict-view-method,old-division,old-ne-operator,round-builtin,no-absolute-import,apply-builtin,hex-method,unicode-builtin,map-builtin-not-iterating,unpacking-in-except,unichr-builtin,old-octal-literal,range-builtin-not-iterating,coerce-builtin,reload-builtin,getslice-method,setslice-method,intern-builtin,oct-method,suppressed-message,raising-string,dict-iter-method,old-raise-syntax,no-else-return,too-many-statements,too-many-branches,inconsistent-return-statements,dangerous-default-value,invalid-name,no-self-use
[REPORTS]
output-format=text
files-output=no
reports=yes
evaluation=10.0 - ((float(5 * error + warning + refactor + convention) / statement) * 10)

[BASIC]
bad-functions=map,filter
good-names=i,j,k,ex,Run,_
bad-names=foo,bar,baz,toto,tutu,tata
name-group=
include-naming-hint=no
module-rgx=(([a-z_][a-z0-9_]*)|([A-Z][a-zA-Z0-9]+))$
module-name-hint=(([a-z_][a-z0-9_]*)|([A-Z][a-zA-Z0-9]+))$
class-attribute-rgx=([A-Za-z_][A-Za-z0-9_]{2,30}|(__.*__))$
class-attribute-name-hint=([A-Za-z_][A-Za-z0-9_]{2,30}|(__.*__))$
class-rgx=[A-Z_][a-zA-Z0-9]+$
class-name-hint=[A-Z_][a-zA-Z0-9]+$
variable-rgx=[a-z_][a-z0-9_]{1,30}$
variable-name-hint=[a-z_][a-z0-9_]{1,30}$
method-rgx=[a-zA-Z_][A-Za-z0-9_]{2,30}$
method-name-hint=[a-z_][a-z0-9_]{2,30}$
inlinevar-rgx=[A-Za-z_][A-Za-z0-9_]*$
inlinevar-name-hint=[A-Za-z_][A-Za-z0-9_]*$
attr-rgx=[A-Za-z0-9_]{2,30}$
attr-name-hint=[a-z_][a-z0-9_]{2,30}$
const-rgx=(([A-Z_][A-Z0-9_]*)|(__.*__))$
const-name-hint=(([A-Z_][A-Z0-9_]*)|(__.*__))$
function-rgx=[a-z_][a-z0-9_]{2,30}$
function-name-hint=[a-z_][a-z0-9_]{2,30}$
argument-rgx=[A-Za-z0-9_]{2,30}$
argument-name-hint=[a-z_][a-z0-9_]{2,30}$
no-docstring-rgx=^_
docstring-min-length=-1

[ELIF]
max-nested-blocks=5

[FORMAT]
max-line-length=120
single-line-if-stmt=yes
no-space-check=trailing-comma,dict-separator
max-module-lines=1000
indent-string='    '
indent-after-paren=4
expected-line-ending-format=

[LOGGING]
logging-modules=logging

[MISCELLANEOUS]
notes=FIXME,XXX,TODO

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=no

[SPELLING]
spelling-dict=
spelling-ignore-words=
spelling-private-dict-file=
spelling-store-unknown-words=no

[TYPECHECK]
ignore-mixin-members=yes
ignored-modules=
ignored-classes=
generated-members=

[VARIABLES]
init-import=no
dummy-variables-rgx=_$|dummy
additional-builtins=
callbacks=cb_,_cb

[CLASSES]
defining-attr-methods=__init__,__new__,setUp
valid-classmethod-first-arg=cls
valid-metaclass-classmethod-first-arg=mcs
exclude-protected=_asdict,_fields,_replace,_source,_make

[DESIGN]
max-args=5
ignored-argument-names=_.*
max-locals=15
max-returns=6
max-branches=12
max-statements=50
max-parents=7
max-attributes=7
min-public-methods=2
max-public-methods=20
max-bool-expr=5

[IMPORTS]
deprecated-modules=optparse
import-graph=
ext-import-graph=
int-import-graph=

[EXCEPTIONS]
overgeneral-exceptions=Exception
